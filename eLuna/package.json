{
  "name": "eluna",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "postcss-run": "postcss ./src/css/imports.css --dir public --watch",
    "test": "jest --env=jsdom"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2"
  },
  "devDependencies": {
    "@testing-library/react": "^13.2.0",
    "@types/jest": "^27.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.24",
    "@types/react-transition-group": "^4.4.4",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.7",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "postcss": "^8.4.13",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.1.0",
    "postcss-preset-env": "^7.5.0",
    "ts-jest": "^28.0.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.7"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
